{
  "name": "@visus-io/notion-sdk-ts",
  "version": "1.0.0",
  "private": false,
  "description": "TypeScript SDK for the Notion API",
  "keywords": [
    "notion",
    "api",
    "sdk",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/visus-io/notion-sdk-ts.git"
  },
  "license": "MIT",
  "author": "Visus Development Team",
  "contributors": [
    {
      "name": "Alan Brault",
      "email": "alan.brault@visus.io"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:html": "eslint . -f html -o reports/lint-report.html",
    "lint:json": "eslint . -f json -o reports/lint-report.json",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@types/node": "^25.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
